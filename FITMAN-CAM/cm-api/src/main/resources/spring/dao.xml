<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="serviceProviderDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.ServiceProvider"/>
    </bean>
    
    <bean id="serviceDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.Service"/>
    </bean>

    <bean id="fiwareMarketplaceDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.FiwareMarketplace"/>
    </bean>

    <bean id="trustedMarketplaceDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.TrustedMarketplace"/>
    </bean>

    <bean id="serviceTemplateDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.ServiceTemplate"/>
    </bean>
    
    <bean id="usdlRepositoryDAO" class="org.kpe.commons.jpa.GenericDAOImpl">
        <constructor-arg value="msee.sp3.cm.domain.UsdlRepository"/>
    </bean>

</beans>
